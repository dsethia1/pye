{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "app.py",  
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "/app.py"   
      },
      {
        "src": "/(.*)",
        "dest": "/Frontend/$1"  
      }
    ]
  }
  